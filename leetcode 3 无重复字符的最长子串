class Solution(object):
    def lengthOfLongestSubstring(self, s):
        """
        :type s: str
        :rtype: int
        """
        left = right = 0
        char_set = set()
        max_len = 0
        current_len = 0
        while (right < len(s)):
            if s[right] not in char_set:
                char_set.add(s[right])
                right += 1
                current_len = right - left
                max_len = max(current_len,max_len)
            else:
                char_set.remove(s[left])
                left += 1
        return max_len
